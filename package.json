{
  "name": "obsidian-metatable",
  "version": "0.13.1",
  "description": "An Obsidian plugin to render the full frontmatter as a table.",
  "keywords": [
    "obsidian"
  ],
  "author": "Arnau Siches",
  "license": "MIT",
  "scripts": {
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "dist": "rm -rf dist && mkdir dist && npm run build && cp styles.css versions.json manifest.json dist/",
    "docs": "npm run dist && cp dist/* docs/.obsidian/plugins/obsidian-metatable",
    "release": "node support/version-bump.mjs",
    "test": "jest --passWithNoTests"
  },
  "devDependencies": {
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.6",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "builtin-modules": "^3.3.0",
    "esbuild": "^0.15.5",
    "esbuild-plugin-solid": "^0.4.2",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "obsidian": "^0.15.9",
    "ts-jest": "^28.0.8",
    "tslib": "^2.0.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "solid-js": "^1.4.7"
  }
}
